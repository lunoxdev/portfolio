---
import Layout from "@layouts/home/layout.astro";

const title = "Moises Obando | iGaming Front End Developer";
const description =
  "Portfolio Web | I love creating immersive and amazing websites that enhance the player experience through carefully crafted code and user-centric design.";
const canonicalUrl = "https://moisesobando.com";
---

<Layout title={title} description={description} canonicalUrl={canonicalUrl}>
  <div
    class="flex flex-col w-full sm:w-1/3 justify-start sm:justify-center"
  >
    <!-- Hero -->
    <section>
      <h1>Moises Obando</h1>
      <h2>iGaming Front-End Developer</h2>
      <br />
      <p>
        Skilled Front-End Developer with 6+ years of experience in web design,
        development, and maintenance, and 4 years in the iGaming industry. I
        love creating immersive and amazing websites that enhance the player
        experience through carefully crafted code and user-centric design.
      </p>
    </section>

    <br />
    <br />

    <!-- Latest Projects -->
    <section>
      <h3>Latest Projects</h3>

      <div class="flex flex-col mt-2 gap-4">
        <!-- Mi casa de apuesta -->
        <a
          href="https://micasadeapuesta.pe/"
          target="_blank"
          rel="noopener noreferrer"
          class="flex flex-col w-full rounded-lg px-4 py-2 space-y-1 border-b border border-[#DCE7FF]/20 hover:bg-gradient-to-bl hover:from-[#DCE7FF]/5 hover:via-[#DCE7FF]/10 hover:to-[#DCE7FF]/15 transition duration-200 ease-in"
        >
          <h3>Mi casa de apuesta</h3>
          <p>Sportsbook Affiliate Site for Peru Market.</p>
        </a>

        <!-- Mobile Legends Counters -->
        <a
          href="https://mobile-legends-counters.vercel.app/"
          target="_blank"
          rel="noopener noreferrer"
          class="flex flex-col w-full rounded-lg px-4 py-2 space-y-1 border-b border border-[#DCE7FF]/20 hover:bg-gradient-to-bl hover:from-[#DCE7FF]/5 hover:via-[#DCE7FF]/10 hover:to-[#DCE7FF]/15 transition duration-200 ease-in"
        >
          <h3>Mobile Legends Counters</h3>
          <p>
            A web app using an ML API to provide hero information helping
            players win by countering enemies effectively.
          </p>
        </a>

        <!-- Other Projects -->
        <div>
          <h4>Other Projects</h4>
          <ul>
            <li>
              <a
                href="https://codesandbox.io/u/lunox"
                target="_blank"
                rel="noopener noreferrer"
                title="Snow Effect project on Sandbox"
                class="text-[#DCE7FF]/90"
              >
                - Snow Effect | Sandbox
              </a>
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="16"
                height="16"
                viewBox="0 0 24 24"
                ><path
                  fill="#DCE7FF"
                  d="M5.616 20q-.691 0-1.153-.462T4 18.384V5.616q0-.691.463-1.153T5.616 4h5.615v1H5.616q-.231 0-.424.192T5 5.616v12.769q0 .23.192.423t.423.192h12.77q.23 0 .423-.192t.192-.423v-5.616h1v5.616q0 .69-.462 1.152T18.384 20zm4.123-5.03l-.708-.709L18.292 5H14V4h6v6h-1V5.708z"
                ></path></svg
              >
            </li>
            <li>
              <a
                href="https://codepen.io/lunox-dev"
                target="_blank"
                rel="noopener noreferrer"
                title="Snow Effect project on CodePen"
                class="text-[#DCE7FF]/90"
              >
                - Snow Effect | CodePen
              </a>
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="16"
                height="16"
                viewBox="0 0 24 24"
                ><path
                  fill="#DCE7FF"
                  d="M5.616 20q-.691 0-1.153-.462T4 18.384V5.616q0-.691.463-1.153T5.616 4h5.615v1H5.616q-.231 0-.424.192T5 5.616v12.769q0 .23.192.423t.423.192h12.77q.23 0 .423-.192t.192-.423v-5.616h1v5.616q0 .69-.462 1.152T18.384 20zm4.123-5.03l-.708-.709L18.292 5H14V4h6v6h-1V5.708z"
                ></path></svg
              >
            </li>
          </ul>
        </div>
      </div>
    </section>

    <br />
    <br />

    <!-- Contact -->
    <section>
      <h3>Connect</h3>
      <p>
        Please feel free to contact me at <strong class="hover:underline"
          >Lunox.code@gmail.com</strong
        >
      </p>

      <!-- Social Media -->
      <div class="flex w-full gap-2 mt-3">
        <!-- GitHub -->
        <a
          href="https://github.com/lunoxdev"
          target="_blank"
          rel="external noopener noreferrer"
          aria-label="Go to GitHub"
          class="group relative inline-flex h-8 w-1/3 items-center justify-center font-medium gap-2 text-[#DCE7FF]/90 border-b border-l border-[#DCE7FF]/20 hover:bg-gradient-to-bl hover:from-[#DCE7FF]/5 hover:via-[#DCE7FF]/10 hover:to-[#DCE7FF]/15 transition duration-200 ease-in rounded-full px-0 py-0 text-xs hover:bg-[#DCE7FF]/10"
